  <JScript><![CDATA[function P_Atividade() {

    this.RPAGOTOENABLE = false;
    this.GotoStepRetryCntMap = {};
    this.GotoBlockMap = {4:[16],8:[9,15],12:[14]};
    this.GotoReverseBlockMap = {16:4,9:8,15:8,14:12};
    this.GotoStepBlockTypeMap = {4:'Loop',16:'EndLoop',8:'Decision',9:'Else',15:'EndDecision',12:'DecisionThen',14:'EndDecisionThen'};


    this.run = function() {

        //------------------------------------------------
        STEP('S81a0ebfb5a2c4a8b9a71dee37d7e0b78', '[1] ExecuteScript');
        // voce pode mudar a localização do excel de destino aqui. 
        // certifique-se de seguir mesma formatação do arquivo original
        this.file_path = 'C:\\Data\\teste.xlsx';

        this.product = {
            name: "",
            price: 0,
            ticket: "",
            pro_link: "",
            date: "",
            site_price: 0
        }

        t_date = new Date();

        this.current_time = t_date.toLocaleDateString('pt-BR', {
                day: 'numeric',
                mouth: 'numeric',
                year: 'numeric'
            });

        //------------------------------------------------
        STEP('S2fe14a09e8ec4fee96d82b281a338c15', '[2] OpenExcel');
        RESULT = StepPlayer.Run('Excel.OpenExcel', this.openexcel_info_2);

        //------------------------------------------------
        STEP('Sa2ffea335c68462984398f8647d25471', '[3] GetRowCount');
        this.row_count = StepPlayer.Run('Excel.GetRowCount', this.getrowcount_info_3);

        //------------------------------------------------
        STEP('Se4b2ea00830543c89117e6b63f8f7099', '[4] For');
        for (this.i = 1; this.i < 5; this.i++) {

            //------------------------------------------------
            STEP('Sdea2da61b925487181471142e9aaa813', '[5] ChromeClose');
            try{
                StepPlayer.Run('Chrome.ChromeClose', this.chromeclose_info_5);
            }catch(e){
                Warn('CatchException: ' + e.message);
            }


            //------------------------------------------------
            STEP('S09d63d9d3fda427bb7475440858c030e', '[6] ReadString');
            this.product_name = StepPlayer.Run('Excel.ReadString', this.readstring_info_6);

            //------------------------------------------------
            STEP('S7e95aa17246a433abdcada2f20679a68', '[7] ExecuteScript');
            this.name_isempety = false;

            // case null or empty
            if(!this.product_name){
                this.name_isempety = true;
                return;
            }

            this.product_name = this.product_name.replace(/\s+/g, "-");


            //------------------------------------------------
            STEP('Sb01899dd9dcc43c9adaaaab1ed7a6f0c', '[8] IfElse');
            if (this.name_isempety === true) {

            //------------------------------------------------
            }else{
                STEP('S1294e846cb7e45fa87588af8fbb093d8', '[9] Else');

                //------------------------------------------------
                STEP('S4520f5ba2c86491bb60615333cf09df3', '[10] GetActiveExcel');
                this.actived_excel = StepPlayer.Run('Excel.GetActiveExcel', this.getactiveexcel_info_10);

                //------------------------------------------------
                STEP('Sd2d8c5a614f84283bc4930fa6caf54a6', '[11] ReadRange');
                this.old_time = StepPlayer.Run('Excel.ReadRange', this.readrange_info_11);

                //------------------------------------------------
                STEP('Sf179dc02da1e444b891e6dc9f1bd3850', '[12] IfThen');
                if (this.old_time !== this.current_time) {

                    //------------------------------------------------
                    STEP('Sa9456650272b4b5e83a3073cf8af312a', '[13] RunTask');
                    RunTask(this, 'T_ReadData', {'product_name':this.product_name, 'current_line':this.i, 'date':this.current_time});

                //------------------------------------------------
                }
                STEP('S66814069ae31455499a502c05b32a3eb', '[14] End-IfThen');

            //------------------------------------------------
            }
            STEP('S1da9a24927094a558b63654f8830a226', '[15] End-IfElse');

        //------------------------------------------------
        }
        STEP('S31dd7ee209fb416da4dacedbc163d388', '[16] End-For');

        //------------------------------------------------
        STEP('Se49b4ed6a8314fad838c2392959d384f', '[17] Debugger');
        OpenDebugger(RESULT);


    }

    this.execute = function() {
        var result = null;
        result = this.run()
        if (result != null)
            return result;
    }
}
]]></JScript>
